
# Default server configuration
#
server {
	server_name nezamichel.rw www.nezamichel.rw;
	root /srv/www/nezamichel.rw;

    listen [::]:443 ssl; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/nezamichel.rw/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/nezamichel.rw/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}


server {
    if ($host = www.nezamichel.rw) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = nezamichel.rw) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


	listen 80;
	listen [::]:80;
	server_name nezamichel.rw www.nezamichel.rw;
    return 404; # managed by Certbot




}